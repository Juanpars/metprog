Algoritmo cuadratica
	Definir a,b,c como Entero;
	Escribir "Ingresa A:";
	Leer a;
	Escribir "Ingresa B";
	Leer b;
	Escribir "Ingresa C";
	Leer c;
	Si b^2-4*a*c >=0 y a  <> 0 Entonces 
	Escribir "La solucion de X1 es: ", (-b-rc(b^2-4*a*c))/(2*a);
	Escribir "La solucion de X2 es: ", (-b+rc(b^2-4*a*c))/(2*a);
SiNo
	Escribir "No Hay Solucion";
FinSi

	
FinAlgoritmo
